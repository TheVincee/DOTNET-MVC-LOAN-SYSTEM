@model List<BKK.Entities.UserType>
    
    <h3>Lista sa mga utangan</h3>
    <a class="btn btn-success" href="Newed/Create">Create New</a>
     <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Operation</th>
    </tr>
  </thead>
  <tbody>
     @foreach(var user in Model){
    <tr>
      <th scope="row">@user.Id</th>
      <td>@user.Name</td>
      
      <td>
          <a class="btn btn-warning" href="/Newed/update/@user.Id">Update</a>
          <a class="btn btn-danger" href="/Newed/delete/@user.Id">Delete</a>
      </td>
      
    </tr>
    }
   
  </tbody>
</table>